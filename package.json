{
  "name": "questionit-space-api",
  "version": "2.0.0",
  "description": "The QuestionIt.space API server built with Nest.js",
  "author": "Louis BÃ©ranger",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:pm2": "tsc-watch -p tsconfig.build.json --onSuccess \"pm2 start -s dist/main.js --max-memory-restart 3500M --no-daemon --name questionitapi --watch dist\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm",
    "cli": "node dist/bootstraps/cli.bootstrap.js",
    "create:migration": "typeorm migration:create",
    "generate:migration": "typeorm migration:generate",
    "run:migration": "typeorm migration:run",
    "revert:migration": "typeorm migration:revert"
  },
  "dependencies": {
    "@jimp/jpeg": "^0.16.1",
    "@jimp/png": "^0.16.1",
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^8.0.3",
    "@types/bull": "^3.15.7",
    "@types/crypto-js": "^4.1.0",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/ioredis": "^4.28.7",
    "@types/multer": "^1.4.7",
    "@types/node-fetch": "2.5.12",
    "@types/passport-jwt": "^3.0.6",
    "@types/uuid": "^8.3.4",
    "@types/web-push": "^3.3.2",
    "bull": "^4.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.1.1",
    "file-type": "16.5.3",
    "fluent-ffmpeg": "^2.1.2",
    "interactive-cli-helper": "^2.0.3",
    "ioredis": "^4.28.4",
    "jimp": "^0.16.1",
    "multer": "^1.4.4",
    "nestjs-throttler-storage-redis": "^0.1.18",
    "node-fetch": "2.6.6",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "random-js": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "tsc-watch": "^4.6.0",
    "twitter-api-v2": "^1.10.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "web-push": "^3.4.5",
    "worker-thread-manager": "^1.1.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
